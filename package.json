{
  "name": "ontario-covid-tracker",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "predev": "rimraf build && yarn graphql-codegen",
    "dev": "razzle start",
    "dev:debug": "razzle start --inspect",
    "prebuild": "yarn graphql-codegen",
    "build": "rimraf build && yarn lint && razzle build",
    "lint": "stylelint \"./src/**/*.(j|t)sx?$\" && tslint \"./src/**/*.{ts,tsx}\" --project ./tsconfig.json && prettier --check \"./src/**/*.{js,ts,tsx,json}\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "prettify-schema": "prettier --write \"src/graphql/schema.graphql\"",
    "test": "razzle test",
    "test:debug": "node --inspect node_modules/jest-cli/bin/jest.js --watch --env=node --runInBand",
    "start": "NODE_ENV=production node build/server.js",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@blueprintjs/core": "^3.15.0",
    "@blueprintjs/icons": "^3.7.0",
    "@blueprintjs/select": "^3.8.0",
    "@lifeomic/axios-fetch": "^1.4.0",
    "@loadable/component": "^5.12.0",
    "apollo-cache": "^1.3.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.15",
    "apollo-link-schema": "^1.2.3",
    "apollo-server-express": "^2.8.1",
    "apollo-utilities": "^1.3.3",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "cleave.js": "^1.4.10",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dataloader": "^2.0.0",
    "dayjs": "^1.8.23",
    "deox": "^2.1.0",
    "express": "^4.16.3",
    "express-boom": "^3.0.0",
    "express-pino-logger": "^4.0.0",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "handsontable": "^10.0.0",
    "immutability-helper": "^3.0.0",
    "json5": "^2.1.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.5.5",
    "morgan": "^1.9.1",
    "ms": "^2.1.1",
    "node-cron": "^2.0.3",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino": "^5.13.1",
    "pino-pretty": "^3.2.0",
    "qs": "^6.5.2",
    "react": "16.8.5",
    "react-apollo": "3.1.3",
    "react-dom": "16.8.5",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "serialize-javascript": "^2.1.1",
    "styled-components": "^4.2.0",
    "ts-optchain": "^0.1.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/fragment-matcher": "^1.12.2",
    "@graphql-codegen/typescript": "^1.5.0",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.5.0",
    "@graphql-codegen/typescript-operations": "^1.5.0",
    "@graphql-codegen/typescript-react-apollo": "^1.5.0",
    "@graphql-codegen/typescript-resolvers": "^1.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cheerio": "^0.22.17",
    "@types/classnames": "^2.2.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/csurf": "^1.9.35",
    "@types/express": "^4.11.1",
    "@types/graphql": "^14.2.2",
    "@types/graphql-iso-date": "^3.3.3",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/loadable__component": "^5.10.0",
    "@types/lodash": "^4.14.118",
    "@types/mongodb": "^3.5.2",
    "@types/morgan": "^1.7.35",
    "@types/ms": "^0.7.30",
    "@types/node": "^11.7.0",
    "@types/node-cron": "^2.0.3",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/pino": "^5.8.8",
    "@types/qs": "^6.5.1",
    "@types/react": "16.8.10",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-test-renderer": "^16.8.2",
    "@types/styled-components": "4.1.8",
    "@types/supertest": "^2.0.7",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.8.0",
    "graphql-tag": "^2.10.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "prettier": "1.16.4",
    "razzle": "^3.0.0",
    "razzle-plugin-scss": "^3.0.0",
    "razzle-plugin-typescript": "^3.0.0",
    "react-test-renderer": "^16.8.6",
    "semantic-release": "^15.13.31",
    "stylelint": "^9.8.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.5.1",
    "typescript": "^3.7.3",
    "webpack": "^4.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kendrickw/ontario-covid-tracker.git"
  }
}
